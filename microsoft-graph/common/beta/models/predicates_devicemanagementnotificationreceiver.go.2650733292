}

func (p DeviceManagementNotificationReceiverOperationPredicate) Matches(input DeviceManagementNotificationReceiver) bool {

	if p.ContactInformation != nil && (input.ContactInformation == nil || *p.ContactInformation != *input.ContactInformation) {
	 	return false
	}


	if p.Locale != nil && (input.Locale == nil || *p.Locale != *input.Locale) {
	 	return false
