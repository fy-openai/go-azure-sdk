type ReferenceUpdateOperationPredicate struct {
	 ODataId *string
	 ODataType *string
}

func (p ReferenceUpdateOperationPredicate) Matches(input ReferenceUpdate) bool {

	if p.ODataId != nil && (input.ODataId == nil || *p.ODataId != *input.ODataId) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	return true
}

