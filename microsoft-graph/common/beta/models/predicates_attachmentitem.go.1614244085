type AttachmentItemOperationPredicate struct {
	 ContentId *string
	 ContentType *string
	 IsInline *bool
	 Name *string
	 ODataType *string
	 Size *int64
}

func (p AttachmentItemOperationPredicate) Matches(input AttachmentItem) bool {

	if p.ContentId != nil && (input.ContentId == nil || *p.ContentId != *input.ContentId) {
	 	return false
	}


	if p.ContentType != nil && (input.ContentType == nil || *p.ContentType != *input.ContentType) {
	 	return false
	}


	if p.IsInline != nil && (input.IsInline == nil || *p.IsInline != *input.IsInline) {
	 	return false
	}


	if p.Name != nil && (input.Name == nil || *p.Name != *input.Name) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	if p.Size != nil && (input.Size == nil || *p.Size != *input.Size) {
	 	return false
	}


	return true
}

