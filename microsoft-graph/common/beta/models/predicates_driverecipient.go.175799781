type DriveRecipientOperationPredicate struct {
	 Alias *string
	 Email *string
	 ODataType *string
	 ObjectId *string
}

func (p DriveRecipientOperationPredicate) Matches(input DriveRecipient) bool {

	if p.Alias != nil && (input.Alias == nil || *p.Alias != *input.Alias) {
	 	return false
	}


	if p.Email != nil && (input.Email == nil || *p.Email != *input.Email) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	if p.ObjectId != nil && (input.ObjectId == nil || *p.ObjectId != *input.ObjectId) {
	 	return false
	}


	return true
}

