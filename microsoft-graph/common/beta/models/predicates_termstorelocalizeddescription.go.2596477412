type TermStoreLocalizedDescriptionOperationPredicate struct {
	 Description *string
	 LanguageTag *string
	 ODataType *string
}

func (p TermStoreLocalizedDescriptionOperationPredicate) Matches(input TermStoreLocalizedDescription) bool {

	if p.Description != nil && (input.Description == nil || *p.Description != *input.Description) {
	 	return false
	}


	if p.LanguageTag != nil && (input.LanguageTag == nil || *p.LanguageTag != *input.LanguageTag) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	return true
}

