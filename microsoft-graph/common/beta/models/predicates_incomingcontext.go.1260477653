type IncomingContextOperationPredicate struct {
	 ODataType *string
	 ObservedParticipantId *string
	 SourceParticipantId *string
}

func (p IncomingContextOperationPredicate) Matches(input IncomingContext) bool {

	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	if p.ObservedParticipantId != nil && (input.ObservedParticipantId == nil || *p.ObservedParticipantId != *input.ObservedParticipantId) {
	 	return false
	}


	if p.SourceParticipantId != nil && (input.SourceParticipantId == nil || *p.SourceParticipantId != *input.SourceParticipantId) {
	 	return false
	}


	return true
}

