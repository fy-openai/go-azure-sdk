type SettingsOperationPredicate struct {
	 HasGraphMailbox *bool
	 HasLicense *bool
	 HasOptedOut *bool
	 ODataType *string
}

func (p SettingsOperationPredicate) Matches(input Settings) bool {

	if p.HasGraphMailbox != nil && (input.HasGraphMailbox == nil || *p.HasGraphMailbox != *input.HasGraphMailbox) {
	 	return false
	}


	if p.HasLicense != nil && (input.HasLicense == nil || *p.HasLicense != *input.HasLicense) {
	 	return false
	}


	if p.HasOptedOut != nil && (input.HasOptedOut == nil || *p.HasOptedOut != *input.HasOptedOut) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	return true
}

