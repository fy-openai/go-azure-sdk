type MarkContentOperationPredicate struct {
	 FontColor *string
	 FontSize *int64
	 Name *string
	 ODataType *string
	 Text *string
}

func (p MarkContentOperationPredicate) Matches(input MarkContent) bool {

	if p.FontColor != nil && (input.FontColor == nil || *p.FontColor != *input.FontColor) {
	 	return false
	}


	if p.FontSize != nil && (input.FontSize == nil || *p.FontSize != *input.FontSize) {
	 	return false
	}


	if p.Name != nil && (input.Name == nil || *p.Name != *input.Name) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	if p.Text != nil && (input.Text == nil || *p.Text != *input.Text) {
	 	return false
	}


	return true
}

