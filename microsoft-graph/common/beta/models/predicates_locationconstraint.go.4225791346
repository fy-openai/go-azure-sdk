type LocationConstraintOperationPredicate struct {
	 IsRequired *bool
	 ODataType *string
	 SuggestLocation *bool
}

func (p LocationConstraintOperationPredicate) Matches(input LocationConstraint) bool {

	if p.IsRequired != nil && (input.IsRequired == nil || *p.IsRequired != *input.IsRequired) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	if p.SuggestLocation != nil && (input.SuggestLocation == nil || *p.SuggestLocation != *input.SuggestLocation) {
	 	return false
	}


	return true
}

