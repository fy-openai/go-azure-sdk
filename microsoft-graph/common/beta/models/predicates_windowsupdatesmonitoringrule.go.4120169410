type WindowsUpdatesMonitoringRuleOperationPredicate struct {
	 ODataType *string
	 Threshold *int64
}

func (p WindowsUpdatesMonitoringRuleOperationPredicate) Matches(input WindowsUpdatesMonitoringRule) bool {

	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	if p.Threshold != nil && (input.Threshold == nil || *p.Threshold != *input.Threshold) {
	 	return false
