type MediaPromptOperationPredicate struct {
	 Loop *int64
	 ODataType *string
}

func (p MediaPromptOperationPredicate) Matches(input MediaPrompt) bool {

	if p.Loop != nil && (input.Loop == nil || *p.Loop != *input.Loop) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	return true
}

