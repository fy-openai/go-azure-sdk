type AttackSimulationRepeatOffenderOperationPredicate struct {
	 ODataType *string
	 RepeatOffenceCount *int64
}

func (p AttackSimulationRepeatOffenderOperationPredicate) Matches(input AttackSimulationRepeatOffender) bool {

	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	if p.RepeatOffenceCount != nil && (input.RepeatOffenceCount == nil || *p.RepeatOffenceCount != *input.RepeatOffenceCount) {
	 	return false
	}


	return true
}

