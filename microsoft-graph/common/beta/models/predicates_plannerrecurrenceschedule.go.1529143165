type PlannerRecurrenceScheduleOperationPredicate struct {
	 NextOccurrenceDateTime *string
	 ODataType *string
	 PatternStartDateTime *string
}

func (p PlannerRecurrenceScheduleOperationPredicate) Matches(input PlannerRecurrenceSchedule) bool {

	if p.NextOccurrenceDateTime != nil && (input.NextOccurrenceDateTime == nil || *p.NextOccurrenceDateTime != *input.NextOccurrenceDateTime) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	if p.PatternStartDateTime != nil && (input.PatternStartDateTime == nil || *p.PatternStartDateTime != *input.PatternStartDateTime) {
	 	return false
	}


	return true
}

