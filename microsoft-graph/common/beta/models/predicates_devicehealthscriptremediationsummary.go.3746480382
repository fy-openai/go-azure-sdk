type DeviceHealthScriptRemediationSummaryOperationPredicate struct {
	 ODataType *string
	 RemediatedDeviceCount *int64
	 ScriptCount *int64
}

func (p DeviceHealthScriptRemediationSummaryOperationPredicate) Matches(input DeviceHealthScriptRemediationSummary) bool {

	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	if p.RemediatedDeviceCount != nil && (input.RemediatedDeviceCount == nil || *p.RemediatedDeviceCount != *input.RemediatedDeviceCount) {
	 	return false
	}


	if p.ScriptCount != nil && (input.ScriptCount == nil || *p.ScriptCount != *input.ScriptCount) {
	 	return false
	}


	return true
}

