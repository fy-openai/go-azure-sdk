type AlertDetectionOperationPredicate struct {
	 DetectionType *string
	 Method *string
	 Name *string
	 ODataType *string
}

func (p AlertDetectionOperationPredicate) Matches(input AlertDetection) bool {

	if p.DetectionType != nil && (input.DetectionType == nil || *p.DetectionType != *input.DetectionType) {
	 	return false
	}


	if p.Method != nil && (input.Method == nil || *p.Method != *input.Method) {
	 	return false
	}


	if p.Name != nil && (input.Name == nil || *p.Name != *input.Name) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	return true
}

