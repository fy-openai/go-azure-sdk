type VpnRouteOperationPredicate struct {
	 DestinationPrefix *string
	 ODataType *string
	 PrefixSize *int64
}

func (p VpnRouteOperationPredicate) Matches(input VpnRoute) bool {

	if p.DestinationPrefix != nil && (input.DestinationPrefix == nil || *p.DestinationPrefix != *input.DestinationPrefix) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	if p.PrefixSize != nil && (input.PrefixSize == nil || *p.PrefixSize != *input.PrefixSize) {
	 	return false
	}


	return true
}

