type MentionsPreviewOperationPredicate struct {
	 IsMentioned *bool
	 ODataType *string
}

func (p MentionsPreviewOperationPredicate) Matches(input MentionsPreview) bool {

	if p.IsMentioned != nil && (input.IsMentioned == nil || *p.IsMentioned != *input.IsMentioned) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	return true
}

