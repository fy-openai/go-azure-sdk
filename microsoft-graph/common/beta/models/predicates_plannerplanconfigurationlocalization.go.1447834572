type PlannerPlanConfigurationLocalizationOperationPredicate struct {
	 Id *string
	 LanguageTag *string
	 ODataType *string
	 PlanTitle *string
}

func (p PlannerPlanConfigurationLocalizationOperationPredicate) Matches(input PlannerPlanConfigurationLocalization) bool {

	if p.Id != nil && (input.Id == nil || *p.Id != *input.Id) {
	 	return false
	}


	if p.LanguageTag != nil && (input.LanguageTag == nil || *p.LanguageTag != *input.LanguageTag) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	if p.PlanTitle != nil && (input.PlanTitle == nil || *p.PlanTitle != *input.PlanTitle) {
	 	return false
	}


	return true
}

