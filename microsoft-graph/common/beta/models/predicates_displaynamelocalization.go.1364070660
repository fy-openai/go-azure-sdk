type DisplayNameLocalizationOperationPredicate struct {
	 DisplayName *string
	 LanguageTag *string
	 ODataType *string
}

func (p DisplayNameLocalizationOperationPredicate) Matches(input DisplayNameLocalization) bool {

	if p.DisplayName != nil && (input.DisplayName == nil || *p.DisplayName != *input.DisplayName) {
	 	return false
	}


	if p.LanguageTag != nil && (input.LanguageTag == nil || *p.LanguageTag != *input.LanguageTag) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	return true
}

