type OptionalClaimOperationPredicate struct {
	 Essential *bool
	 Name *string
	 ODataType *string
	 Source *string
}

func (p OptionalClaimOperationPredicate) Matches(input OptionalClaim) bool {

	if p.Essential != nil && (input.Essential == nil || *p.Essential != *input.Essential) {
	 	return false
	}


	if p.Name != nil && (input.Name == nil || *p.Name != *input.Name) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	if p.Source != nil && (input.Source == nil || *p.Source != *input.Source) {
	 	return false
	}


	return true
}

