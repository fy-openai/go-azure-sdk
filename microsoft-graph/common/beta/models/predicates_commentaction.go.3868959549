type CommentActionOperationPredicate struct {
	 IsReply *bool
	 ODataType *string
}

func (p CommentActionOperationPredicate) Matches(input CommentAction) bool {

	if p.IsReply != nil && (input.IsReply == nil || *p.IsReply != *input.IsReply) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	return true
}

