type MimeContentOperationPredicate struct {
	 ODataType *string
	 Type *string
	 Value *string
}

func (p MimeContentOperationPredicate) Matches(input MimeContent) bool {

	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	if p.Type != nil && (input.Type == nil || *p.Type != *input.Type) {
	 	return false
	}


	if p.Value != nil && (input.Value == nil || *p.Value != *input.Value) {
	 	return false
	}


	return true
}

