type SecurityIpEvidenceOperationPredicate struct {
	 CountryLetterCode *string
	 CreatedDateTime *string
	 IpAddress *string
	 ODataType *string
	 RemediationStatusDetails *string
}

func (p SecurityIpEvidenceOperationPredicate) Matches(input SecurityIpEvidence) bool {

	if p.CountryLetterCode != nil && (input.CountryLetterCode == nil || *p.CountryLetterCode != *input.CountryLetterCode) {
	 	return false
	}


	if p.CreatedDateTime != nil && (input.CreatedDateTime == nil || *p.CreatedDateTime != *input.CreatedDateTime) {
	 	return false
	}


	if p.IpAddress != nil && (input.IpAddress == nil || *p.IpAddress != *input.IpAddress) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	if p.RemediationStatusDetails != nil && (input.RemediationStatusDetails == nil || *p.RemediationStatusDetails != *input.RemediationStatusDetails) {
	 	return false
	}


	return true
}

