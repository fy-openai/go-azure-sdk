type DataStoreFieldOperationPredicate struct {
	 Name *string
	 ODataType *string
	 Searchable *bool
	 Unique *bool
}

func (p DataStoreFieldOperationPredicate) Matches(input DataStoreField) bool {

	if p.Name != nil && (input.Name == nil || *p.Name != *input.Name) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	if p.Searchable != nil && (input.Searchable == nil || *p.Searchable != *input.Searchable) {
	 	return false
	}


	if p.Unique != nil && (input.Unique == nil || *p.Unique != *input.Unique) {
	 	return false
	}


	return true
}

