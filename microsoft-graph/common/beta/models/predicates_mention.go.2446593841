type MentionOperationPredicate struct {
	 Application *string
	 ClientReference *string
	 CreatedDateTime *string
	 DeepLink *string
	 Id *string
	 MentionText *string
	 ODataType *string
	 ServerCreatedDateTime *string
}

func (p MentionOperationPredicate) Matches(input Mention) bool {

	if p.Application != nil && (input.Application == nil || *p.Application != *input.Application) {
	 	return false
	}


	if p.ClientReference != nil && (input.ClientReference == nil || *p.ClientReference != *input.ClientReference) {
	 	return false
	}


	if p.CreatedDateTime != nil && (input.CreatedDateTime == nil || *p.CreatedDateTime != *input.CreatedDateTime) {
	 	return false
	}


	if p.DeepLink != nil && (input.DeepLink == nil || *p.DeepLink != *input.DeepLink) {
	 	return false
	}


	if p.Id != nil && (input.Id == nil || *p.Id != *input.Id) {
	 	return false
	}


	if p.MentionText != nil && (input.MentionText == nil || *p.MentionText != *input.MentionText) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	if p.ServerCreatedDateTime != nil && (input.ServerCreatedDateTime == nil || *p.ServerCreatedDateTime != *input.ServerCreatedDateTime) {
	 	return false
	}


	return true
}

