type SimulationOperationPredicate struct {
	 AutomationId *string
	 CompletionDateTime *string
	 CreatedDateTime *string
	 Description *string
	 DisplayName *string
	 DurationInDays *int64
	 Id *string
	 IsAutomated *bool
	 LastModifiedDateTime *string
	 LaunchDateTime *string
	 ODataType *string
}

func (p SimulationOperationPredicate) Matches(input Simulation) bool {

	if p.AutomationId != nil && (input.AutomationId == nil || *p.AutomationId != *input.AutomationId) {
	 	return false
	}


	if p.CompletionDateTime != nil && (input.CompletionDateTime == nil || *p.CompletionDateTime != *input.CompletionDateTime) {
	 	return false
	}


	if p.CreatedDateTime != nil && (input.CreatedDateTime == nil || *p.CreatedDateTime != *input.CreatedDateTime) {
	 	return false
	}


	if p.Description != nil && (input.Description == nil || *p.Description != *input.Description) {
	 	return false
	}


	if p.DisplayName != nil && (input.DisplayName == nil || *p.DisplayName != *input.DisplayName) {
	 	return false
	}


	if p.DurationInDays != nil && (input.DurationInDays == nil || *p.DurationInDays != *input.DurationInDays) {
	 	return false
	}


	if p.Id != nil && (input.Id == nil || *p.Id != *input.Id) {
	 	return false
	}


	if p.IsAutomated != nil && (input.IsAutomated == nil || *p.IsAutomated != *input.IsAutomated) {
	 	return false
	}


	if p.LastModifiedDateTime != nil && (input.LastModifiedDateTime == nil || *p.LastModifiedDateTime != *input.LastModifiedDateTime) {
	 	return false
	}


	if p.LaunchDateTime != nil && (input.LaunchDateTime == nil || *p.LaunchDateTime != *input.LaunchDateTime) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	return true
}

