type AttributeMappingParameterSchemaOperationPredicate struct {
	 AllowMultipleOccurrences *bool
	 Name *string
	 ODataType *string
	 Required *bool
}

func (p AttributeMappingParameterSchemaOperationPredicate) Matches(input AttributeMappingParameterSchema) bool {

	if p.AllowMultipleOccurrences != nil && (input.AllowMultipleOccurrences == nil || *p.AllowMultipleOccurrences != *input.AllowMultipleOccurrences) {
	 	return false
	}


	if p.Name != nil && (input.Name == nil || *p.Name != *input.Name) {
	 	return false
	}


	if p.ODataType != nil && (input.ODataType == nil || *p.ODataType != *input.ODataType) {
	 	return false
	}


	if p.Required != nil && (input.Required == nil || *p.Required != *input.Required) {
	 	return false
	}


	return true
}

